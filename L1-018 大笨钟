#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>

int main()
{
	int i,j,h,m;
	printf("Only");
	scanf("%d&d", &h,&m);
	if (h <= 24 && h >= 12)
	{
		for (i = 12; i < h; i++)
		{
			printf("å½“");
		}
		if (m != 0)
		{
			printf("dang");
		}
	}
	else
	{
		printf("Only %d:d.  Too early to Dang.", h, m);
	}
	
	return 0;
}
