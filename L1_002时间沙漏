#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
#include<windows.h>
int main()
{
	int i = 0, n = 0, j;
	char a[] =  "     " ;
	char b[] =  "#####" ;
	char c[] = "#####";
	j = strlen(a) - 1;
	while (i < 3)
	{
		printf("%s\n", b);
		b[i] = a[i];
		b[j - i] = a[j - i];
		i++;
	}
	for(i=0;i<2;i++)
	{
		c[n] = a[3];
		c[j - n] = a[3];
		if (i == 1)
		{
			c[0] = c[4] = c[3];
		}
		printf("%s\n", c);
	}
	return 0;
}
